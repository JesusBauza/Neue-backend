{
  "kind": "collectionType",
  "collectionName": "proyectos",
  "info": {
    "singularName": "proyecto",
    "pluralName": "proyectos",
    "displayName": "Proyecto",
    "mainField": "Id_titulo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Id_titulo": {
      "type": "string"
    },
    "Titulo": {
      "type": "text",
      "required": true
    },
    "servicios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::servicio.servicio",
      "mappedBy": "proyectos"
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Home_Cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Info": {
      "type": "blocks"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Uid": {
      "type": "uid",
      "targetField": "Titulo"
    },
    "Credito": {
      "type": "component",
      "component": "cred.cred",
      "repeatable": true
    },
    "categoria": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    },
    "related_works": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::proyecto.proyecto",
      "inversedBy": "proyectos"
    },
    "proyectos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::proyecto.proyecto",
      "mappedBy": "related_works"
    },
    "Year": {
      "type": "integer"
    }
  }
}